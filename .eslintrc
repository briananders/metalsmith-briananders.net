{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "globals": {
    "describe": false,
    "it": false,
    "before": false,
    "after": false,
    "afterEach": false,
    "document": true,
    "window": true,
    "browser": true,
    "dataLayer": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/no-dynamic-require": 0,
    "import/newline-after-import": 0,
    "lines-around-directive": 0,
    "arror-parens": 0,
    "no-prototype-builtins": 0,
    "no-plusplus": 0,
    "global-require": 1,
    "strict": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-await-in-loop": 0,
    "no-multi-spaces": 1,
    "no-param-reassign": 0,
    "prefer-destructuring": [1, {
      "array": false,
      "object": true
    }, {
      "enforceForRenamedProperties": false
    }],
    "comma-dangle": ["error", {
      "arrays": "never",
      "objects": "always-multiline",
      "imports": "never",
      "exports": "never",
      "functions": "ignore"
    }],
    "no-confusing-arrow": [
      "warn",
      {
        "allowParens": true
      }
    ]
  }
}